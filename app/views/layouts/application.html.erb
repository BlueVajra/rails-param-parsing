<!DOCTYPE html>
<html>
<head>
  <title>Rails Params</title>
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<header>
  <div class="container">
    <div class="logo">
      <%= link_to "Simple", root_path %>
      |
      <%= link_to "Nested", nested_path %>
      |
      <%= link_to "Deeply Nested", deeply_nested_path %>
      |
      <%= link_to "Array", array_path %>
      |
      <%= link_to "Hash", hash_path %>
    </div>
  </div>
</header>
<main>
  <div class="container">
    <%= yield %>
  </div>
</main>
<footer>
  <div class="container"></div>
</footer>
</body>
</html>
